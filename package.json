    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack-env": "^1.15.2",
    "audiobuffer-to-wav": "^1.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "css-loader": "^4.2.1",
    "del-cli": "^3.0.1",
